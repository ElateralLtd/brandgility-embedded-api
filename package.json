{
  "name": "@elateral/brandgility-embedded-api",
  "version": "0.1.2",
  "description": "Brandgility embedded mode library",
  "repository": "https://github.com/ElateralLtd/brandgility-embedded-api.git",
  "main": "lib/brandgility-embedded-api.js",
  "scripts": {
    "build": "webpack --env.production",
    "test": "concurrently \"yarn run test:lint\" \"yarn run test:unit\"",
    "test:lint": "eslint src --cache --max-warnings=0 --report-unused-disable-directives",
    "test:unit": "jest",
    "test:unit:coverage": "jest --coverage",
    "test:unit:watch": "yarn run test:unit --watch",
    "ci:test": "yarn run ci:test:lint && yarn run ci:test:unit",
    "ci:test:coverage": "yarn run ci:test:lint && yarn run ci:test:unit:coverage",
    "ci:test:lint": "yarn run test:lint",
    "ci:test:unit": "yarn run test:unit --runInBand --ci",
    "ci:test:unit:coverage": "yarn run ci:test:unit --coverage && yarn run coveralls",
    "deploy": "yarn publish --access public --new-version $npm_package_version",
    "update-master-if-version-is-increased": "node ./bin/update-master-if-version-is-increased.js",
    "coveralls": "minicat reports/test/coverage/lcov.info | coveralls"
  },
  "jest-junit": {
    "output": "reports/test/junit.xml"
  },
  "moduleRoots": [
    "src"
  ],
  "license": "UNLICENSED",
  "engines": {
    "node": ">=12.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.4.3",
    "@babel/plugin-proposal-class-properties": "^7.4.0",
    "@babel/plugin-proposal-export-default-from": "^7.2.0",
    "@babel/plugin-proposal-export-namespace-from": "^7.2.0",
    "@babel/plugin-proposal-private-methods": "^7.4.4",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/preset-env": "^7.4.3",
    "@commitlint/cli": "^7.5.2",
    "@commitlint/is-ignored": "^7.5.1",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.8.0",
    "babel-loader": "^8.0.4",
    "babel-plugin-dynamic-import-node": "^2.2.0",
    "concurrently": "^4.1.0",
    "core-js": "^3.1.3",
    "coveralls": "^3.0.3",
    "dotenv": "^7.0.0",
    "eslint": "^5.16.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-import-resolver-webpack": "^0.11.1",
    "eslint-plugin-import": "^2.17.3",
    "eslint-plugin-jest": "^22.4.1",
    "eslint-plugin-unicorn": "^8.0.2",
    "husky": "^1.3.1",
    "jest": "^24.8.0",
    "jest-junit": "^6.3.0",
    "minicat": "^1.0.0",
    "semver": "^5.6.0",
    "terser-webpack-plugin": "^1.2.2",
    "unused-files-webpack-plugin": "^3.4.0",
    "webpack": "^4.30.0",
    "webpack-cli": "^3.3.1"
  }
}
